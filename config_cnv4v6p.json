{
    "input": [
        {
            "type": "maxmindGeoLite2CountryCSV",
            "action": "add",
            "args": {
                "country": "./geolite2/GeoLite2-Country-Locations-en.csv",
                "ipv4": "./geolite2/GeoLite2-Country-Blocks-IPv4.csv",
                "ipv6": "./geolite2/GeoLite2-Country-Blocks-IPv6.csv"
            }
        },
        {
            "type": "cutter",
            "action": "remove",
            "args": {
                "wantedList": [
                    "cn"
                ],
                "onlyIPType": "ipv4"
            }
        },
        {
            "type": "text",
            "action": "add",
            "args": {
              "name": "cnv4",
              "uri": "https://raw.githubusercontent.com/17mon/china_ip_list/master/china_ip_list.txt",
              "onlyIPType": "ipv4"
            }
          },
        {
            "type": "text",
            "action": "add",
            "args": {
                "inputDir": "./data"
            }
        },
        {
            "type": "private",
            "action": "add"
        }
    ],
    "output": [
        {
            "type": "v2rayGeoIPDat",
            "action": "output",
            "args": {
                "outputName": "cn-ipv6-private.dat",
                "wantedList": [
                    "cn",
                    "private"
                ]
            }
        },
        {
            "type": "v2rayGeoIPDat",
            "action": "output",
            "args": {
                "outputName": "cn-ipv4-ipv6-private.dat",
                "wantedList": [
                    "cn",
                    "cnv4",
                    "private"
                ]
            }
        },
        {
            "type": "maxmindMMDB",
            "action": "output",
            "args": {
                "outputName": "cn-ipv6-private.mmdb",
                "wantedList": [
                    "cn",
                    "private"
                ]
            }
        },
        {
            "type": "maxmindMMDB",
            "action": "output",
            "args": {
                "outputName": "cn-ipv4-ipv6-private.mmdb",
                "wantedList": [
                    "cn",
                    "cnv4",
                    "private"
                ]
            }
        }
    ]
}